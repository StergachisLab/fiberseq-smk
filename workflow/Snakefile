rule all:
    input:
        "b.txt",


rule example_rule:
    input:
        "a.txt",
    output:
        "b.txt",
    resources:
        mem=config.get("mem", 16),
    threads: config.get("threads", 16)
    conda:
        "envs/env.yml"
    log:
        "logs/example_rule.log",
    params:
        script=workflow.source_path("scripts/a.py"),
    shell:
        """
        """
